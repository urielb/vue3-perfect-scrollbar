{
    "name": "vue3-perfect-scrollbar",
    "version": "1.7.1",
    "description": "Vue.js wrapper for perfect scrollbar",
    "type": "module",
    "paths": {
        "source": "src",
        "destination": "dist",
        "example": "example"
    },
    "main": "dist/vue3-perfect-scrollbar.cjs.js",
    "module": "dist/vue3-perfect-scrollbar.esm.js",
    "style": "dist/vue3-perfect-scrollbar.css",
    "types": "./types/vue3-perfect-scrollbar.d.ts",
    "scripts": {
        "build": "rollup -c --environment production",
        "build:css": "postcss src/style.css -o dist/vue3-perfect-scrollbar.min.css --env production && postcss src/style.css -o dist/vue3-perfect-scrollbar.css",
        "dev": "webpack-dev-server --config webpack.config.js",
        "test": "yarn jest",
        "test:dev": "node --inspect-brk node_modules/.bin/jest --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mercs600/vue3-perfect-scrollbar"
    },
    "author": "Adam Marcinkowski <developer@mercs600.pl>",
    "keywords": [
        "frontend",
        "scroll",
        "scrollbar",
        "vue"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@eslint/js": "^9.39.2",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@vue/compiler-sfc": "^3.0.2",
        "@vue/test-utils": "^2.0.0-beta.7",
        "babel-jest": "^30.2.0",
        "babel-loader": "^10.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.39.2",
        "eslint-plugin-n": "^17.16.2",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-vue": "^10.6.2",
        "eslint-webpack-plugin": "^5.0.2",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "postcss": "^8.4.31",
        "postcss-cli": "^11.0.1",
        "rollup": "^4.53.5",
        "sass": "^1.97.0",
        "sass-loader": "^16.0.6",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.3",
        "vue": "^3.0.2",
        "vue-eslint-parser": "^10.1.0",
        "vue-jest": "^5.0.0-alpha.5",
        "vue-loader": "^17.4.2",
        "vue-style-loader": "^4.1.2",
        "webpack": "^5.104.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2"
    },
    "dependencies": {
        "cssnano": "^7.1.2",
        "perfect-scrollbar": "^1.5.5",
        "postcss-import": "^16.1.1"
    },
    "resolutions": {
        "loader-utils": ">=1.4.2",
        "follow-redirects": ">=1.15.6",
        "braces": ">=3.0.3",
        "micromatch": ">=4.0.8",
        "semver": ">=7.5.2",
        "on-headers": ">=1.1.0",
        "js-yaml": ">=3.14.2"
    },
    "volta": {
        "node": "12.22.12"
    }
}
